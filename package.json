{
  "name": "nftx-holdings-subgraph-mainnet",
  "version": "0.1.0",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "prepare-mainnet": "mustache config/ethereum-mainnet.json subgraph.template.yaml > subgraph.yaml",
    "prepare-rinkeby": "mustache config/ethereum-rinkeby.json subgraph.template.yaml > subgraph.yaml",
    "prepare-arbitrum": "mustache config/arbitrum-one.json subgraph.template.yaml > subgraph.yaml",
    "create-local": "graph create --node http://localhost:8020/ quantumlyy/nftx-holdings-subgraph-mainnet",
    "deploy-mainnet-hosted": "graph deploy --product hosted-service nftx-project/nftx-holdings-subgraph-mainnet",
    "deploy-rinkeby": "graph deploy --product hosted-service quantumlyy/nftx-holdings-subgraph-rinkeby",
    "deploy-one": "graph deploy --product hosted-service quantumlyy/nftx-holdings-subgraph-arbitrum",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 quantumlyy/nftx-holdings-subgraph-mainnet",
    "remove-local": "graph remove --node http://localhost:8020/ kodexlabs/exchange-subgraph",
    "deploy-mainnet": "graph deploy --studio nftx-token-holdings"
  },
  "dependencies": {},
  "devDependencies": {
    "@amxx/graphprotocol-utils": "^1.1.0-alpha.2",
    "@graphprotocol/graph-cli": "^0.28.1",
    "@graphprotocol/graph-ts": "0.26.0",
    "@openzeppelin/contracts": "^4.5.0",
    "@openzeppelin/subgraphs": "^0.1.7-1",
    "@sushiswap/core": "^1.4.2",
    "mustache": "^4.2.0"
  },
  "resolutions": {
    "colors": "1.4.0",
    "@graphprotocol/graph-ts": "0.26.0"
  }
}
